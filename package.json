    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:server:prod": "ng run pk2-pro-angular:server && webpack --config webpack.server.config.js --progress --colors",
    "build:browser:prod": "ng build --prod",
    "build:prod": "npm run build:browser:prod && npm run build:server:prod",
    "server": "node local.js",
    "build:browser:serverless": "ng build --prod --base-href /production/",
    "build:serverless": "npm run build:browser:serverless && npm run build:server:serverless",
    "build:prod:deploy": "npm run build:prod && npm run deploy",
    "build:serverless:deploy": "npm run build:serverless && npm run deploy",
    "deploy": "serverless deploy",
    "build:server:serverless": "ng run pk2-pro-angular:server:serverless && webpack --config webpack.server.config.js --progress --colors",
    "build:serverless:dev": "npm run build:browser:serverless:dev && npm run build:server:serverless:dev",
    "build:browser:serverless:dev": "ng build --configuration=dev --base-href /dev/",
    "build:server:serverless:dev": "ng run pk2-pro-angular:server:serverlessDEV && webpack --config webpack.server.config.js --progress --colors",
    "build:serverless:uat": "npm run build:browser:serverless:uat && npm run build:server:serverless:uat",
    "build:browser:serverless:uat": "ng build --configuration=uat --base-href /uat/",
    "build:server:serverless:uat": "ng run pk2-pro-angular:server:serverlessUAT && webpack --config webpack.server.config.js --progress --colors",
    "build:serverless:beta": "npm run build:browser:serverless:beta && npm run build:server:serverless:beta",
    "build:browser:serverless:beta": "ng build --configuration=beta --base-href /beta/",
    "build:server:serverless:beta": "ng run pk2-pro-angular:server --configuration=serverlessBETA && webpack --config webpack.server.config.js --progress --colors"
